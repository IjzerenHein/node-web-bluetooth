# EditorConfig: http://editorconfig.org

root = true

[*]
indent_style = tab
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.js]
indent_style = tab
indent_size = 2
quote_type = single

[*.yml]
indent_style = space
indent_size = 2
quote_type = single
